<%-include('partials/header')%>
<div class="content">
  <h1>Compose Blog</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="blogTitle" />
      <label>Blog</label>
      <textarea
        class="form-control"
        name="blogBody"
        rows="5"
        cols="30"
      ></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>
</div>
<%-include('partials/footer')%>
